<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDFFFF"
    tools:context=".ViewStudentProfile">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="match_parent"
        android:layout_height="247dp"
        android:layout_marginLeft="-40dp"
        android:layout_marginTop="-21dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/v7" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="189dp"
        android:layout_height="153dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="120dp"
        android:background="@android:drawable/picture_frame"/>


    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="190dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="STUDENT"
        android:textColor="#4CAF50"
        android:textSize="38sp"
        android:textStyle="italic" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="match_parent"
        android:layout_height="88dp"
        android:layout_marginTop="205dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="PROFILE"
        android:textColor="#4CAF50"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="67dp"
        android:layout_height="69dp"
        android:layout_marginLeft="172dp"
        android:layout_marginTop="130dp"
        app:srcCompat="@drawable/dp" />

    <ScrollView
        android:layout_width="355dp"
        android:layout_height="313dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="270dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="20dp">

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Student ID"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Section"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Name"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Username"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Password"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Email"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"
                />

            <TextView
                android:id="@+id/textView33"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Contact Number"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"/>

            <TextView
                android:id="@+id/textView34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Address"
                android:textColor="#F24CAF50"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tvStudentDetails8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#F2172218"
                android:textSize="16sp"
                android:background="?attr/editTextBackground"
                android:backgroundTint="#009688"/>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>